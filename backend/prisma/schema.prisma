generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Candle {
  id        Int      @id @default(autoincrement())
  symbol    String
  interval  String
  openTime  DateTime
  open      Float
  high      Float
  low       Float
  close     Float
  volume    Float
}

